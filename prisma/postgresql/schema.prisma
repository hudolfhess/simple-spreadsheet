generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/@prisma/postgresql"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_DATABASE_URL")
}

model SpreadSheet {
  id            String   @id @default(cuid())
  name          String
  updatedAt     DateTime @default(now())
  createdAt     DateTime @default(now())
}
